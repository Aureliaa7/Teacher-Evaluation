@model TeacherEvaluation.Application.Pages.Shared.InsertFromFileBaseModel

<div class="jumbotron register-jumbotron">
    <form method="post" enctype="multipart/form-data">
        <div class="row my-row">
            <div class="col-md-4">
                <input asp-for="ExcelFile" multiple class="form-control custom-file-input" />
                <label class="custom-file-label">Choose document</label>
                <span asp-validation-for="ExcelFile" class="text-danger"></span>
            </div>
        </div>

        <div class="row my-row">
            <div class="col-md-4">
                <br />
                <input type="submit" value="Add" class="btn btn-dark form-control" />
            </div>
        </div>
    </form>
    <div class="row my-row">
        <div class="col-md-8">
            @if (Model.ErrorMessages.Count > 0)
            {
                @foreach (var error in Model.ErrorMessages)
                {
                    <span class="field-validation-error">@error</span>
                    <br />
                    <br />
                }
            }
        </div>
    </div>
</div>